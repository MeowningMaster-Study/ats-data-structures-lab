{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Build",
			"command": "C:/msys64/mingw64/bin/g++.exe",
			"args": [
				"-fdiagnostics-color=always",
				"-std=c++17",
				"-g",
				"${workspaceFolder}/src/main.cpp",
				"-o",
				"${workspaceFolder}/main.exe"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
		}
	]
}